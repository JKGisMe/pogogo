{{#poke-card
onsubmit=attrs.onsubmit
title="Register"
buttonText="Register"}}
  <div class="row">
    {{poke-input
    classNames="col s12"
    type="email"
    label="Email Address"}}
  </div>
  <div class="row">
    {{poke-input
    classNames="col s12"
    type="password"
    label="Password"}}
  </div>
  <div class="row">
    {{poke-input
    classNames="col s12"
    type="password"
    label="Confirm Password"}}
  </div>
{{/poke-card}}
